#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/doctoc@2.2.1/node_modules/doctoc/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/doctoc@2.2.1/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/doctoc@2.2.1/node_modules/doctoc/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/doctoc@2.2.1/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../doctoc/doctoc.js" "$@"
else
  exec node  "$basedir/../doctoc/doctoc.js" "$@"
fi
