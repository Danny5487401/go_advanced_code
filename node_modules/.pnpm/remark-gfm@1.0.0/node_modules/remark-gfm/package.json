{
  "name": "remark-gfm",
  "version": "1.0.0",
  "description": "remark plugin to support GFM (autolink literals, strikethrough, tables, tasklists)",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "table",
    "strikethrough",
    "tasklist",
    "autolink",
    "github",
    "gfm"
  ],
  "repository": "remarkjs/remark-gfm",
  "bugs": "https://github.com/remarkjs/remark-gfm/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "types": "types/index.d.ts",
  "files": [
    "types/index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "mdast-util-gfm": "^0.1.0",
    "micromark-extension-gfm": "^0.3.0"
  },
  "devDependencies": {
    "dtslint": "^4.0.0",
    "is-hidden": "^1.0.0",
    "not": "^0.1.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "remark": "^13.0.0-alpha.1",
    "remark-cli": "^8.0.0",
    "remark-preset-wooorm": "^7.0.0",
    "string-width": "^4.2.0",
    "tape": "^5.0.0",
    "to-vfile": "^6.0.0",
    "unified": "^9.0.0",
    "xo": "^0.33.0"
  },
  "scripts": {
    "format": "remark . -qfo --ignore-pattern test/ && prettier . --write && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test-types": "dtslint types",
    "test": "npm run format && npm run test-coverage && npm run test-types"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prefer-optional-catch-binding": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
