#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules/commitizen/bin/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules/commitizen/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules/commitizen/bin/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules/commitizen/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules:/Users/python/Downloads/git_download/go_advanced_code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules/commitizen/bin/commitizen" "$@"
else
  exec node  "$basedir/../../../../../commitizen@4.3.0_@types+node@20.11.17_typescript@5.3.3/node_modules/commitizen/bin/commitizen" "$@"
fi
